#include <bits/stdc++.h>
using namespace std;

const int INF = 1e9;
int a[4] = {1, 2, 7, 9};
int dp[16][2];

int solve(int m, int s) {
    int &res = dp[m][s];
    if (res != -1) return res;

 
    if (m == 15)
        return res = (s == 1 ? 0 : INF);

    res = INF;
    vector<int> v;

    for (int i = 0; i < 4; i++) {
        if (((m >> i) & 1) == s)
            v.push_back(i);
    }

 
    for (int i : v) {
        res = min(res, a[i] + solve(m ^ (1 << i), s ^ 1));
    }

 
    if (s == 0) {
        for (int i = 0; i < (int)v.size(); i++) {
            for (int j = i + 1; j < (int)v.size(); j++) {
                int x = v[i], y = v[j];
                int cost = max(a[x], a[y]);
                res = min(res,
                          cost + solve(m ^ (1 << x) ^ (1 << y), s ^ 1));
            }
        }
    }

    return res;
}

int main() {
    memset(dp, -1, sizeof(dp));
    cout << solve(0, 0) << '\n';
    return 0;
}
