#include <bits/stdc++.h>
using namespace std;

static const long long MOD = 1000000009;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
    }

    vector<long long> d(n + 1, 0);
    d[0] = 1;
    if (n >= 1) d[1] = 0;

    for (int i = 2; i <= n; i++) {
        d[i] = ((i - 1) * (d[i - 1] + d[i - 2])) % MOD;
    }

    cout << d[n] << '\n';
    return 0;
}
